<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="MealBase">
    <defName>MealBagOfDicks</defName>
    <label>Bag of Dicks</label>
    <description>A high-quality packaged meal, made for 2yys. Great for traveling.</description>
    <graphicData>
      <texPath>Food/bagofdicks</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <DeteriorationRate>0.25</DeteriorationRate>
      <MarketValue>24</MarketValue>
      <Mass>0.3</Mass>
      <WorkToMake>450</WorkToMake>
      <Nutrition>0.9</Nutrition>
    </statBases>
    <ingestible>
      <preferability>MealFine</preferability>
      <tasteThought>AteBagOfDicks</tasteThought>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
  </ThingDef>
  
  <ThingDef ParentName="MealBase">
    <defName>MealPlateOfDicks</defName>
    <label>Plate of Dicks</label>
    <description>A Lavish Quality meal. Great for a cheap mood buff if you can handle resorting to eating a plate of dicks.</description>
    <graphicData>
      <texPath>Food/plateofdicks</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <DeteriorationRate>0.25</DeteriorationRate>
      <MarketValue>34</MarketValue>
      <Mass>0.3</Mass>
      <WorkToMake>450</WorkToMake>
      <Nutrition>1.4</Nutrition>
    </statBases>
    <ingestible>
      <preferability>MealLavish</preferability>
      <tasteThought>AtePlateOfDicks</tasteThought>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
  </ThingDef>
</Defs>